<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs.kangwon.com.teamsw" >
    <uses-permission android:name = "android.permission.BLUETOOTH"/>
    <uses-permission android:name = "android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:largeHeap="true">
        <activity
            android:name= "cs.kangwon.com.indian.Game"
            android:label = "TeamSw">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.MainActivity"
            android:label = "TeamSw">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.Search"
            android:label = "TeamSw"
            android:theme="@android:style/Theme.Holo.Dialog">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.Help2"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:label = "TeamSw">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.Help"
            android:label = "TeamSw">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.Select"
            android:label = "TeamSw">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.Card"
            android:label = "TeamSw">
        </activity>
        <activity
            android:name= "cs.kangwon.com.indian.ResultView"
            android:label = "TeamSw">
        </activity>
    </application>

</manifest>
